query ($fullName : String
       $phoneNumber : String
       $createdAt : Date ){
       allCustomersWithFilters(fullName: $fullName
                                  phoneNumber : $phoneNumber
                                  createdAt : $createdAt) {
                                                            id
                                                            fullName
                                                            phoneNumber
                                                            address
                                                            createdAt
                                                          }
}